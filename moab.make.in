MOAB_LIBDIR = @abs_builddir@/src/.libs
MOAB_INCLUDEDIR = @abs_srcdir/src@ -I@abs_builddir/src@

MOAB_CXXFLAGS = @CXXFLAGS@ @AM_CXXFLAGS@
MOAB_CFLAGS = @CFLAGS@ @AM_CFLAGS@
MOAB_LDFLAGS = @EXPORT_LDFLAGS@

MOAB_INCLUDES = -I$(MOAB_INCLUDEDIR)

MOAB_LIBS_LINK = ${MOAB_LDFLAGS} -L${MOAB_LIBDIR} -lMOAB @LIBS@ @CGM_LIBS@ @NETCDF_LIBS@ @HDF5_LIBS@
