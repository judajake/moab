IMESH_CXXFLAGS = @CXXFLAGS@ 
IMESH_CFLAGS = @CFLAGS@
IMESH_LDFLAGS = @LDFLAGS@

IMESH_FC = @FC@
IMESH_FCFLAGS = @FCFLAGS@

IMESH_FCDEFS = @DEFS@

IMESH_CBIND_INCLUDES = -I$(IMESH_INCLUDEDIR)

IMESH_CBIND_LIBS_LINK = $(IMESH_LDFLAGS) -L$(MOAB_LIBDIR) -L${IMESH_LIBDIR} -liMesh -lMOAB @LIBS@

IMESH_INCLUDEDIR = @abs_srcdir@
IMESH_LIBDIR = @abs_builddir@/.libs
MOAB_LIBDIR = @abs_builddir@/../../.libs
